<div class="pt-20">
  <div class="alert alert-danger pt-20" role="alert" hidden="true">Select at least one player to use for comparison</div>
  <options-panel key="options-panel" selected=state.selected on-add-player("addPlayer")/>
  <div class="pt-20"/>
  <div class="block-container no-padding">
    <players-table key="players-table" players=input.players selected=state.selected on-player-checked("playerChecked")/>
  </div>
  <new-player-form player=state.player/>
</div>
